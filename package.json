{
  "name": "smart-cache",
  "version": "1.0.1",
  "description": "Cache invalidation tools for apollo-client's in-memory-cache.",
  "bin": "build/cli.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.js",
  "scripts": {
    "build": "tsc && rollup --config && cp build/codegen.js* lib/",
    "typings": "tsc -d --emitDeclarationOnly --declarationDir typings",
    "release": "release-it",
    "test": "cross-env TS_NODE_CACHE=false TS_NODE_FILES=true mocha -r ts-node/register test/**/*.spec.ts"
  },
  "keywords": [
    "apollo-client",
    "cache"
  ],
  "author": "SmartX",
  "contributors": [
    "Ji Ye <yeji@smartx.com>",
    "Yanzhen Yu <yanzhen@smartx.com>"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "es",
    "lib",
    "build",
    "typings"
  ],
  "peerDependencies": {
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "graphql": "^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/graphql": "^14.2.3",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-schema": "^1.2.3",
    "apollo-utilities": "^1.3.2",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "mocha": "^6.1.4",
    "release-it": "^12.3.2",
    "rollup": "^1.16.7",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
